{
  "name": "vite-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/vite-plugin/src",
  "projectType": "library",
  "tags": ["npm:public", "type:tooling", "scope:vite"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "cwd": "packages/vite-plugin",
        "command": "vite build"
      },
      "cache": true,
      "inputs": [
        "production",
        "^production",
        {
          "externalDependencies": ["vite"]
        }
      ],
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "dependsOn": ["^typecheck"],
      "options": {
        "cwd": "packages/vite-plugin",
        "command": "tsc --build --emitDeclarationOnly"
      },
      "cache": true
    },
    "test": {
      "executor": "nx:run-script",
      "options": {
        "script": "test"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/vite-plugin",
        "command": "eslint ."
      },
      "cache": true
    }
  }
}
