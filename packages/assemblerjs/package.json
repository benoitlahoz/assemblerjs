{
  "name": "assemblerjs",
  "description": "A general purpose Dependency Injection library for node and browser.",
  "version": "1.0.1",
  "author": "Beno√Æt LAHOZ <info@benoitlahoz.io>",
  "bugs": "https://github.com/benoitlahoz/assemblerjs/issues",
  "homepage": "https://github.com/benoitlahoz/assemblerjs#README",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benoitlahoz/assemblerjs.git"
  },
  "keywords": [
    "agencement",
    "assemblage",
    "assembler",
    "composition",
    "decorator",
    "dependency",
    "di",
    "injection",
    "ioc",
    "machine",
    "metadata",
    "multi",
    "reflection",
    "swc",
    "typescript",
    "reflect-metadata"
  ],
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">= 18.12.0"
  },
  "files": [
    "dist/**/*",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "test": "vitest --coverage --passWithNoTests",
    "bench": "vitest bench",
    "coverage": "vitest run --coverage --passWithNoTests && istanbul-badges-readme && node scripts/update-performance-badges.mjs"
  },
  "dependencies": {
    "@assemblerjs/core": "^0.9.6",
    "reflect-metadata": "0.2.2"
  },
  "devDependencies": {
    "istanbul-badges-readme": "^1.9.0",
    "vitest": "^3.0.0"
  }
}
